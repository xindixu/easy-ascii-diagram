{
  "plugins": ["prettier", "react-hooks"],
  "extends": ["airbnb", "prettier", "prettier/react"],
  "env": {
    "node": true,
    "es6": true,
    "browser": true
  },
  "rules": {
    "semi": "error",
    "react/jsx-one-expression-per-line": "off",
    "react/destructuring-assignment": "off",
    "space-infix-ops": "error",
    "prettier/prettier": ["error"]
  },
  "parser": "babel-eslint"
}